                                            @if (member?.AvatarData != null)
                                            {
                                                <img alt="Image placeholder"
                                             class="avatar avatar-xs me-2"
                                             src="data:image/*;base64,@(Convert.ToBase64String(member.AvatarData))" />
                                            }
                                            else
                                            {
                                                <img src="https://cdn.icon-icons.com/icons2/1378/PNG/512/avatardefault_92824.png"
                                             alt="Image placeholder"
                                             class="avatar avatar-xs me-2" />
                                            }
                                            <span> @member.FullName </span>
                                        </div>
                                    </td>
                                    <td class="text-xs font-weight-bold">
                                        @{
                                            var memberRole = (await RoleService.GetUserRolesAsync(member));

                                        }
                                        <span class="my-2 text-xs"> @memberRole.First() </span>
                                    </td>
                                    <td class="text-xs font-weight-bold">
                                        <span class="my-2 text-xs"> @member.Email </span>
                                    </td>
                                    <td class="text-xs font-weight-bold">
                                        <span class="my-2 text-xs"> @member.PhoneNumber </span>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
